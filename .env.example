# Environment variables for Korea Maps MCP Server Docker setup
# Copy this file to .env and fill in your actual values

# Kakao API Key (Required)
# Get your API key from https://developers.kakao.com/
KAKAO_REST_API_KEY=your_kakao_rest_api_key_here

# Optional: Cache and Rate Limiting Configuration
MCP_KAKAO_CACHE_TTL=3600
MCP_KAKAO_RATE_LIMIT_CALLS=10
MCP_KAKAO_RATE_LIMIT_PERIOD=1
MCP_KAKAO_CONCURRENCY_LIMIT=5

# Optional: Server Configuration (for HTTP transports)
MCP_TRANSPORT=stdio
MCP_HOST=127.0.0.1
MCP_PORT=8000
MCP_PATH=/mcp
MCP_LOG_LEVEL=INFO
